{
  "expo": {
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/ihelp/notification.png",
          "color": "black"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Access to your photo library is required to attach photos to networking post or change profile images or portfolio images."
        }
      ]
    ],
    "name": "ihelp",
    "slug": "ihelphr",
    "version": "0.2.2",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splashscreen.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },

    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "mn.ihelphrnovelist",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Access to your photo library is required to attach photos to networking post or change profile images or portfolio images.",
        "NSCameraUsageDescription": "Access to your camera to networking post or change profile images or portfolio images."
      }
    },

    "android": {
      "versionCode": 11,
      "package": "mn.ihelphrnovelist",
      "permissions": ["CAMERA", "WRITE_EXTERNAL_STORAGE", "CAMERA_ROLL"],
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#FFFFFF"
      }
    }
  }
}
